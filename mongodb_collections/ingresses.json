[
  {
    "_id": "demo-v72-ingress",
    "service_name": "demo-v72",
    "ingress_name": "demo-v72-ingress",
    "metadata": {
      "name": "demo-v72-ingress",
      "namespace": "demo-v72",
      "annotations": {
        "nginx.ingress.kubernetes.io/rewrite-target": "/",
        "nginx.ingress.kubernetes.io/ssl-redirect": "true",
        "cert-manager.io/cluster-issuer": "letsencrypt-prod",
        "external-dns.alpha.kubernetes.io/hostname": "jollicow.store"
      }
    },
    "spec": {
      "tls": [
        {
          "hosts": [
            "jollicow.store"
          ],
          "secretName": "demo-v72-tls"
        }
      ],
      "rules": [
        {
          "host": "jollicow.store",
          "http": {
            "paths": [
              {
                "path": "/api",
                "pathType": "Prefix",
                "backend": {
                  "service": {
                    "name": "demo-v72-service",
                    "port": {
                      "number": 80
                    }
                  }
                }
              }
            ]
          }
        }
      ]
    },
    "version": 1,
    "created_at": "2025-10-15T10:30:00Z",
    "updated_at": "2025-10-15T10:30:00Z"
  },
  {
    "_id": "demo-v72-gateway",
    "service_name": "demo-v72",
    "ingress_name": "demo-v72-gateway",
    "metadata": {
      "name": "demo-v72-gateway",
      "namespace": "demo-v72",
      "annotations": {
        "nginx.ingress.kubernetes.io/ssl-redirect": "false",
        "nginx.ingress.kubernetes.io/use-regex": "true",
        "nginx.ingress.kubernetes.io/rewrite-target": "/api/$2"
      }
    },
    "spec": {
      "ingressClassName": "nginx",
      "rules": [
        {
          "host": "gateway.local",
          "http": {
            "paths": [
              {
                "path": "/api/demo-v72/api(/|$)(.*)",
                "pathType": "ImplementationSpecific",
                "backend": {
                  "service": {
                    "name": "demo-v72-service",
                    "port": {
                      "number": 80
                    }
                  }
                }
              }
            ]
          }
        },
        {
          "host": "localhost",
          "http": {
            "paths": [
              {
                "path": "/api/demo-v72/api(/|$)(.*)",
                "pathType": "ImplementationSpecific",
                "backend": {
                  "service": {
                    "name": "demo-v72-service",
                    "port": {
                      "number": 80
                    }
                  }
                }
              }
            ]
          }
        }
      ]
    },
    "version": 1,
    "created_at": "2025-10-15T10:30:00Z",
    "updated_at": "2025-10-15T10:30:00Z"
  }
]