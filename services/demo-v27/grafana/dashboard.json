{
  "dashboard": {
    "id": null,
    "uid": "demo-v27",
    "title": "demo-v27 API Dashboard",
    "tags": [
      "demo-v27",
      "flask",
      "api",
      "microservice"
    ],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Requests & Memory",
        "type": "barchart",
        "targets": [
          {
            "expr": "demo-v27_memory_usage_bytes{instance=\"host.docker.internal:5000\"} / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "A"
          },
          {
            "expr": "demo-v27_active_connections{instance=\"host.docker.internal:5000\"}",
            "legendFormat": "Active Connections",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Response Time & Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "demo-v27_response_time_seconds{instance=\"host.docker.internal:5000\"}",
            "legendFormat": "Response Time (s)",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Service Status & Requests",
        "type": "barchart",
        "targets": [
          {
            "expr": "up{instance=\"host.docker.internal:5000\"}",
            "legendFormat": "demo-v27 Status",
            "refId": "A"
          },
          {
            "expr": "demo-v27_requests_total{instance=\"host.docker.internal:5000\"}",
            "legendFormat": "Total Requests",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 0
  }
}